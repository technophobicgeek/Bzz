
<div class="toolbar">
  <div class="leftItem regularButton">
    <a href="<%= url_for :action => :end_session %>">End</a>
  </div>
</div>


<script src="/public/js/application.js" type="text/javascript"></script>


<!-- dummy link for ajax calls -->
<div id="#dummy"></div>


<div id="controls" class="content">
  <% ['High','Med','Low','Off'].each do |cmd| %>
    <div class="controlpanelButton">
      <a href="#dummy" onClick="setIntensity('<%= cmd.downcase %>');"><%= cmd %></a>
    </div>
  <% end %>
</div>
