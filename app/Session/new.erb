<div class="pageTitle">
  <h1>New <%= @session.title %></h1>
</div>

<div class="toolbar">
  <div class="leftItem backButton">
    <a class="cancel" href="<%= url_for :action => :index %>">Cancel</a>
  </div>
</div>

<div class="content">
  <form method="POST" action="<%= url_for :action => :create %>">
    <input type="hidden" name="id" value="<%= @session.object %>"/>
    <ul>
      
          <li>
            <label for="session[id]" class="fieldLabel">Id</label>
            <input type="text" name="session[id]" <%=
            placeholder("Id") %> />
          </li>
      
          <li>
            <label for="session[name]" class="fieldLabel">Name</label>
            <input type="text" name="session[name]" <%=
            placeholder("Name") %> />
          </li>
      
          <li>
            <label for="session[create_ts]" class="fieldLabel">Create ts</label>
            <input type="text" name="session[create_ts]" <%=
            placeholder("Create ts") %> />
          </li>
      
          <li>
            <label for="session[use_ts]" class="fieldLabel">Use ts</label>
            <input type="text" name="session[use_ts]" <%=
            placeholder("Use ts") %> />
          </li>
      
          <li>
            <label for="session[rating]" class="fieldLabel">Rating</label>
            <input type="text" name="session[rating]" <%=
            placeholder("Rating") %> />
          </li>
      
          <li>
            <label for="session[sequence]" class="fieldLabel">Sequence</label>
            <input type="text" name="session[sequence]" <%=
            placeholder("Sequence") %> />
          </li>
      
          <li>
            <label for="session[notes]" class="fieldLabel">Notes</label>
            <input type="text" name="session[notes]" <%=
            placeholder("Notes") %> />
          </li>
      
    </ul>
    <input type="submit" class="standardButton" value="Create"/>
  </form>
</div>